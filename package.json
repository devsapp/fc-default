{
  "name": "fc-default",
  "version": "0.0.15",
  "description": "This is a component demo for Serverless Devs Tool ",
  "keywords": [
    "Serverless",
    "Framework",
    "Serverless-Devs",
    "Serverless-Tool",
    "Function Compute"
  ],
  "homepage": "https://www.serverless-devs.com",
  "author": "Serverless-Devs",
  "contributors": [],
  "license": "MIT",
  "main": "./lib/index.js",
  "publishConfig": {
    "access": "public"
  },
  "autoInstall": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/ServerlessTool/express-alibaba"
  },
  "scripts": {
    "start": "npm run watch",
    "watch": "tsc -w",
    "publish": "npm run build && s cli platform publish",
    "prebuild": "npm install rimraf && rimraf package-lock.json && rimraf lib && rimraf node_modules && npm i",
    "build": "ncc build src/index.ts -m -e @serverless-devs/core -o lib",
    "postbuild": "rm -rf package-lock.json",
    "doc": "npx typedoc src/index.ts --json doc/doc.json --out doc",
    "fix": "f2elint fix",
    "lint": "f2elint scan"
  },
  "dependencies": {
    "@serverless-devs/core": "0.0.149",
    "hostile": "^1.3.3",
    "js-yaml": "^4.0.0",
    "os": "^0.1.0"
  },
  "devDependencies": {
    "@types/eslint": "^7.2.6",
    "@types/node": "^14.0.23",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "@vercel/ncc": "^0.24.0",
    "eslint": "^7.7.0",
    "f2elint": "^1.2.0",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.2",
    "typedoc": "^0.20.35",
    "typescript": "^4.4.2"
  }
}
